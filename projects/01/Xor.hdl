/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=a, b=notB, out=aNotB);
    And(a=notA, b=b, out=bNotA);
    Or(a=aNotB, b=bNotA, out=out);
}
